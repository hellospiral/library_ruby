<h2>Library Books</h2>
<hr>

<ul class="list-group">
  <% @books.each() do |book| %>
    <li class="list-group-item"><a href="/books/<%=book.bookid()%>"><%= book.title() %></a></li>
  <% end %>
</ul>

<div class="btn-group" role="group">
  <a href="/" class="btn btn-default" role="button">Home</a>
  <a href="/book/new" class="btn btn-warning active" role="button">Add New Book</a>
</div>
